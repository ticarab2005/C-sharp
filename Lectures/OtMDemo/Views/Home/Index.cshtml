@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
<h1>Roster</h1>
<div class="row">
    <div class="col-6">
        <partial name="_TeacherForm"/>
        <partial name="_StudentForm"/>
    </div>
    <div class="col-6">
        <h2>All Teachers</h2>
        @{
            foreach(Teacher t in ViewBag.AllTeachers)
            {
                <h4>Instructors @t.Name - @t.Course</h4>
                @* <h4>Instructors @t.TeacherId</h4> *@
            }
        }
    </div>
</div>
</div>
